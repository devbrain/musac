<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google//DTD GWT 2.8//EN"
    "http://gwtproject.org/doctype/2.8.0/gwt-module.dtd">
    
<module rename-to="musac">
    <!-- Inherit core GWT -->
    <inherits name="com.google.gwt.user.User"/>
    
    <!-- Source paths -->
    <source path="src/com/musac/gwt"/>
    
    <!-- Public resources (JavaScript files) -->
    <public path="build"/>
    
    <!-- Include JavaScript files -->
    <script src="musac_wasm.js"/>
    <script src="musac.js"/>
    
    <!-- Entry point (if needed for testing) -->
    <!-- <entry-point class="com.musac.gwt.client.MusacEntryPoint"/> -->
</module>