#ifndef MUSAC_SDK_CONFIG_H
#define MUSAC_SDK_CONFIG_H

// Endianness configuration
#cmakedefine IS_BIG_ENDIAN

#ifdef IS_BIG_ENDIAN
    #define MUSAC_BIG_ENDIAN 1
    #define MUSAC_LITTLE_ENDIAN 0
#else
    #define MUSAC_BIG_ENDIAN 0
    #define MUSAC_LITTLE_ENDIAN 1
#endif

// Platform detection
#cmakedefine CMAKE_SYSTEM_NAME "@CMAKE_SYSTEM_NAME@"

// Version information (if needed in future)
#cmakedefine MUSAC_SDK_VERSION_MAJOR @MUSAC_SDK_VERSION_MAJOR@
#cmakedefine MUSAC_SDK_VERSION_MINOR @MUSAC_SDK_VERSION_MINOR@
#cmakedefine MUSAC_SDK_VERSION_PATCH @MUSAC_SDK_VERSION_PATCH@

#endif // MUSAC_SDK_CONFIG_H